{
    "randomState": 42,

    "predictands": {
        "predictandsFolder": "F:\\TFM\\predictands",
        "station": 1,
        "hisFile": "F:\\recibido_JaviG\\corrientes_bahia\\validacion_2011\\Santander_his.nc",
        "removeTimesteps": 24,
        "variables": ["u_x", "u_y", "waterlevel"],
        "sigmaLayer": -0.1,
        "resample": "mean"
    },

    "predictors": {
        "predictorsFolder": "F:\\TFM\\predictors",
        "wind": "meteogalicia",
        "hydro": {
            "dataset_id": "cmems_mod_ibi_phy_my_0.083deg-2D_PT1H-m",
            "point": [43.6, -3.742],
            "variables": ["ubar", "vbar", "zos"]

        },
        "tidalRange": "F:\\TFM_input\\REDMAR_Santander_PdE.mat"
    },

    "preprocess": {
        "trainTestSplit": {
            "method": "temporal",
            "testSize": 0.2
        },
        "scale": {
            "method": "standard"
        },
        "dimReduction": {
            "method": "isomap",
            "nComponents": 5
        }
    },

    "model": {
        "method": "analogues",

        "analogues": {
            "clustering": "spectral",
            "nAnalogues": 500,
            "regressor": "knn",
            "knn": {
                "n_neighbors": 5,
                "weights": "distance",
                "metric": "minkowski"
            },
            "krr": {
                "kernel": "rbf",
                "alpha": 1,
                "gamma": null
            }
        },

        "adaBoost": {
            "nSplits": 5,
            "estimator": {
                "maxDepth": [3, 5, 7, 9],
                "criterion": ["squared_error"],
                "splitter": ["best"],
                "minSamplesSplit": [2],
                "minSamplesLeaf": [1]
            },
            "nEstimators": [30, 50, 100, 150],
            "learningRate": [0.00001, 0.0001, 0.001, 0.01, 0.1],
            "loss": ["square", "exponential", "linear"],
            "scoring": "neg_mean_squared_error",
            "nJobs": -1
        },

        "lstm": {
            "nTimesteps": 10,
            "stepSize": 1,
            "lstmLayers": {
                "minLstmLayers": 1,
                "maxLstmLayers": 3,
                "minLstmUnits": 50,
                "maxLstmUnits": 200,
                "stepLstmUnits": 50
            },
            "dropout": {
                "minDropout": 0.1,
                "maxDropout": 0.5,
                "stepDropout": 0.1
            },
            "denseLayers": {
            "minDenseLayers": 1,
            "maxDenseLayers": 3,
            "minDenseUnits": 50,
            "maxDenseUnits": 200,
            "stepDenseUnits": 50
            },
            "train": {
                "optimizer": "adam",
                "loss": "mean_squared_error",
                "metrics": ["mean_squared_error", "mean_absolute_error"],
                "learningRates": [0.00001, 0.0001, 0.001, 0.01],
                "earlyStopping": {
                    "monitor": "val_loss",
                    "patience": 15
                },
                "batch": {
                    "minBatchSize": 32,
                    "maxBatchSize": 128,
                    "stepBatchSize": 32
                }
            },
            "hyperband": {
                "objective": "val_loss",
                "direction": "min",
                "maxEpochs": 200,
                "factor": 3,
                "overwrite": true,
                "directory": "ktuner",
                "projectName": "lstm"
            }
        }
    }
    
}